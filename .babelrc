{
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env"
        ],
        "@ava/babel/stage-4",
        "@babel/preset-react"
      ]
    }
  },
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "ignore": [
    "node_modules/**"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-react-jsx"
  ]
}
