{
  "name": "@elricb/ink-control-components",
  "version": "0.3.2",
  "description": "Decision components for Ink",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elricb/ink-control-components.git"
  },
  "author": "elricb (github.com/elricb)",
  "main": "lib",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "babel src/ -d lib/",
    "release": "npm run build && npm run lint && npm test && npm publish",
    "example": "node lib/example/control.js && node lib/example/http.js",
    "lint": "xo",
    "test": "ava",
    "test-update-snapshots": "ava --update-snapshots"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "ink",
    "component",
    "jsx",
    "react"
  ],
  "prettier": "@elricb/prettier-config",
  "ava": {
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/preset-react"
        ]
      }
    }
  },
  "babelpresets": [
    "@ava/babel/stage-4",
    "@babel/preset-react"
  ],
  "babel": {
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "bugs": {
    "url": "https://github.com/elricb/ink-control-components/issues"
  },
  "homepage": "https://github.com/elricb/ink-control-components#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "devDependencies": {
    "@ava/babel": "^2.0",
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@elricb/prettier-config": "0.0.1",
    "@elricb/xo-config": "^0.0.4",
    "ava": "^4.0.1",
    "eslint-config-xo-react": "^0.26.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "ink": "^3.2.0",
    "ink-testing-library": "^2.0.0",
    "ink-use-stdout-dimensions": "^1.0.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "xo": "^0.47.0"
  },
  "peerDependencies": {
    "ink": ">=3",
    "prop-types": ">=14",
    "react": ">=16"
  },
  "dependencies": {
    "ink-text-input": "^4.0.2"
  }
}
