{
  "name": "@elricb/ink-control-components",
  "version": "0.1.2",
  "description": "Decision components for Ink",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elricb/ink-control-components.git"
  },
  "author": "elricb (github.com/elricb)",
  "main": "lib",
  "engines": {
    "node": ">=11"
  },
  "scripts": {
    "build": "babel src/ -d lib/",
    "release": "npm run build && npm run lint && npm test && npm publish",
    "example": "node lib/example/index.js",
    "lint": "xo",
    "test": "ava",
    "test-update-snapshots": "ava --update-snapshots"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "ink",
    "component",
    "jsx",
    "react"
  ],
  "peerDependencies": {
    "ink": "^3.0.5",
    "react": "^16"
  },
  "prettier": "@elricb/prettier-config",
  "ava": {
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/preset-react"
        ]
      }
    }
  },
  "babelpresets": [
    "@ava/babel/stage-4",
    "@babel/preset-react"
  ],
  "babel": {
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "bugs": {
    "url": "https://github.com/elricb/ink-control-components/issues"
  },
  "homepage": "https://github.com/elricb/ink-control-components#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "devDependencies": {
    "@ava/babel": "^2.0",
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@elricb/prettier-config": "0.0.1",
    "@elricb/xo-config": "0.0.1",
    "ava": "^3.15.0",
    "eslint-config-xo-react": "^0.25.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "ink": "^3.0.5",
    "ink-testing-library": "^2.0.0",
    "ink-use-stdout-dimensions": "^1.0.5",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "react": "^16.14.0",
    "xo": "^0.44.0"
  },
  "dependencies": {
    "ink-text-input": "^4.0.1"
  }
}
